{
  "name": "crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "postcss src/assets/css/tailwind.css -o src/assets/css/main.css",
    "watch:css": "postcss src/assets/css/tailwind.css -o src/assets/css/main.css",
    "start": "npm run watch:css & (cd ../core && npm i && npm run build) && env-cmd react-scripts start",
    "build": "npm run build:css & (cd ../core && npm i && npm run build) && env-cmd react-scripts build",
    "build:ci": "CI=true npm run build",
    "test": "react-scripts test",
    "test:ci": "CI=true npm test",
    "lint": "eslint 'src/**/*.tsx' 'src/**/*.ts'",
    "lint-fix": "eslint \"./src/**\" --fix",
    "eject": "react-scripts eject"
  },
